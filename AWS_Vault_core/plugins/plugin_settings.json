            "on_icon_clicked": {
              "import_bgeo": "Import geo to Current file",
              "preview_bgeo": "Preview geo in Gplay"
            }
          }
        },
        {
          "files": [ "hip", "hipnc", "hiplc" ],
          "methods": {
            "on_get": null,
            "on_save": null,
            "on_lock": null,
            "on_icon_clicked": {
              "merge_hip": "Merge Hip File",
              "open_hip": "Open Hip file"
            }
          }
        }
      ]
    },
    {
      "enable": "True",
      "software": [ "maya" ],
      "script": "maya.py",
      "binding": [ ]
    }
  ]
}


