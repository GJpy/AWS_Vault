      "software": [ "houdinicore", "hescape", "hindie", "houdinifx", "houdini" ],
      "script": "houdini.py",
      "binding": [
        {
          "files": [ "bgeo", "bgeo.gz", "bgeogz" ],
          "methods": {
            "on_get": "reload_geo",
